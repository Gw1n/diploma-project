[master]
${master_ip} ansible_user=ubuntu ansible_ssh_private_key_file=${ssh_key_path}

[workers]
${worker_ip} ansible_user=ubuntu ansible_ssh_private_key_file=${ssh_key_path}

[srv]
${srv_ip} ansible_user=ubuntu ansible_ssh_private_key_file=${ssh_key_path}

[all:vars]
ansible_python_interpreter=/usr/bin/python3
ansible_ssh_common_args='-o StrictHostKeyChecking=no'